{
  "manifest_version": 2,

  "name": "EBA Dark Mode",
  "version": "0.2.1",
  "description": "Dark mode for EBA",
  "content_scripts": [
    {
      "matches": ["https://*.eba.gov.tr/*"],
      "js": ["eba.js"]
    }
  ],
  "web_accessible_resources": ["eba.css"],
  "permissions": ["tabs", "<all_urls>"]
}
